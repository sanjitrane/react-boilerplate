{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating apps with React.js, Redux, Server-side rendering support and Unit testing with Jest",
  "main": "index.js",
  "repository": "git@github.com:sanjitrane/react-boilerplate.git",
  "author": "Sanjit Rane",
  "license": "MIT",
  "engines": {
    "node": "7.10.1"
  },
  "scripts": {
    "build": "webpack --config webpack.build.config.js",
    "server-build": "webpack --config webpack.server.config.js",
    "start-node": "node server.js",
    "start:dev": "webpack-dev-server",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "css-loader": "^0.28.11",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.14.2",
    "lodash": "^4.17.19",
    "node-sass": "^4.8.3",
    "react": "^16.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-config": "^5.0.1",
    "react-select": "^3.0.4",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fetch-mock": "^7.3.9",
    "jest": "^24.8.0",
    "moxios": "^0.4.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "./src/setUpTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/webpack/test.js"
    ]
  },
  "homepage": "."
}
